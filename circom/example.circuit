pragma circom 2.0.0;

template Multiplier(n) {
    signal input a;
    signal input b;
    signal output c[n];

    signal int[n];

    int[0] <== a*a + b;
    c[0] <== int[0];

    for (var i=1; i<n; i++) {
    int[i] <== int[i-1]*int[i-1] + b;
    c[i] <== int[i];
    }
    
}

component main = Multiplier(3);
